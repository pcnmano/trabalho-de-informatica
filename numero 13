int main()
{
    int n1, n2, n3, mf, v1, v2, v3, s;
    
    printf("Informe a nota 1 do aluno:");
    scanf("%d", &n1);
    
    printf("Informe a nota 2 do aluno:");
    scanf("%d", &n2);
    
    printf("Informe a nota 3 do aluno:");
    scanf("%d", &n3);
    
    v1 = (n1 * 2);
    v2 = (n2 * 3);
    v3 = (n3 * 5);
    
    s = v1 + v2 + v3;
    mf = (s/10);
    
    printf("A média final do aluno é: %d\n", mf);
    return 0;
}
